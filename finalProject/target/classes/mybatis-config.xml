<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>	
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.kh.evening.board.model.vo.Board" alias="Board"/>
		<typeAlias type="com.kh.evening.board.model.vo.Reply" alias="Reply"/>
     	<typeAlias type="com.kh.evening.board.model.vo.Attachment" alias="Attachment"/>
		<typeAlias type="com.kh.evening.gesipan.model.vo.Gesipan" alias="Gesipan"/>
		<typeAlias type="com.kh.evening.board.model.vo.BoardMode" alias="BoardMode"/>
		<typeAlias type="com.kh.evening.gesipan.model.vo.GesipanReply" alias="gReply"/>
		<typeAlias type="com.kh.evening.member.model.vo.Member" alias="Member"/>
		<typeAlias type="com.kh.evening.payment.model.vo.Payment" alias="Payment"/>
		<typeAlias type="com.kh.evening.board.model.vo.AuctionHistory" alias="AuctionHistory"/>
		<typeAlias type="com.kh.evening.board.model.vo.GoodLike" alias="GoodLike"/>
		<typeAlias type="com.kh.evening.member.model.vo.RankCode" alias="RankCode"/>
		<typeAlias type="com.kh.evening.message.model.vo.Message" alias="Message"/>
	</typeAliases>
	
	<mappers>
      	<mapper resource="mapper/board-mapper.xml"/>
		<mapper resource="mapper/gesipan-mapper.xml"/>
		<mapper resource="mapper/memberMapper.xml" />
		<mapper resource="mapper/pay-mapper.xml"/>
		<mapper resource="mapper/message-mapper.xml"/>
	</mappers>
</configuration>
